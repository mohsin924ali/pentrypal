{
  "name": "pentrypal-next",
  "version": "1.0.0",
  "description": "A rock-solid React Native app for collaborative grocery and pantry management",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "test:e2e:ios": "detox test --configuration=ios.sim.debug",
    "test:e2e:android": "detox test --configuration=android.emu.debug",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "generate:icons": "npx @expo/vector-icons-cli",
    "analyze:bundle": "npx expo export --source-maps && npx expo-bundle-analyzer dist/bundles",
    "clean": "rm -rf node_modules && npm install",
    "clean:cache": "npx expo start --clear",
    "postinstall": "patch-package && husky install",
    "prepare": "husky install",
    "validate": "npm run type-check && npm run lint:check && npm run format:check && npm run test:ci"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "@reduxjs/toolkit": "^2.2.7",
    "babel-plugin-module-resolver": "^5.0.2",
    "base-64": "^1.0.0",
    "crypto-js": "^4.2.0",
    "expo": "~51.0.39",
    "expo-application": "~5.9.1",
    "expo-blur": "~13.0.2",
    "expo-constants": "~16.0.2",
    "expo-crypto": "~13.0.2",
    "expo-device": "~6.0.2",
    "expo-file-system": "^18.1.11",
    "expo-font": "~12.0.10",
    "expo-haptics": "~13.0.1",
    "expo-image": "~1.13.0",
    "expo-image-picker": "~15.1.0",
    "expo-intent-launcher": "^12.1.5",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-local-authentication": "~14.0.1",
    "expo-localization": "~15.0.3",
    "expo-media-library": "^17.1.7",
    "expo-notifications": "~0.28.17",
    "expo-secure-store": "~13.0.2",
    "expo-sharing": "^13.1.5",
    "expo-splash-screen": "~0.27.6",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-updates": "~0.25.24",
    "i18next": "^23.14.0",
    "lottie-react-native": "6.7.0",
    "nativewind": "^4.1.23",
    "react": "18.2.0",
    "react-hook-form": "^7.53.0",
    "react-i18next": "^15.0.1",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-mmkv": "^3.0.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-view-shot": "^4.0.3",
    "react-redux": "^9.1.2",
    "redux-persist": "^6.0.0",
    "zod": "^3.23.8",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@expo/cli": "^0.18.28",
    "@react-native/eslint-config": "^0.74.87",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.7.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.79",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "detox": "^20.27.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-detox": "^1.0.0",
    "eslint-plugin-jest": "^28.8.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.4",
    "lint-staged": "^15.2.9",
    "patch-package": "^8.0.0",
    "prettier": "^3.3.3",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "private": true,
  "expo": {
    "install": {
      "exclude": [
        "react-native-safe-area-context"
      ]
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
